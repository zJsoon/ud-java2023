package Clase_05;

import javax.swing.JFrame;
import javax.swing.JTextArea;

public class VentanaListado extends JFrame{
	private JTextArea txtArea;
	
	public VentanaListado() {
		super();
		setBounds(200, 300, 600, 400);
		
		txtArea = new JTextArea();
		getContentPane()
		
		setVisible(true);
	}
}
